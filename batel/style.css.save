.icon {
	z-index:999999;
	position: fixed; 
	width:35px;
	height:35px;
	border-radius:20px;
	padding:0.2em;
	background-color:#d2004f;
	cursor:pointer;
	margin:50px 100px ;
}

.hamburger{
	width:20px;
	height:2px;
	background:#ffffff;
	position:absolute;
	margin:17px 17px ;
	margin-bottom:4px;
	transform:translate(-50%, -50%);
	box-shadow: 0 2px 5px rgba(0,0,0,.2);
	transition: .5s;
	line-height: 35px;
}

.hamburger:before,
.hamburger:after
{

	content:'';
	position:absolute;
	width:20px;
	height:2px;
	background:#ffffff;
	box-shadow:0 0px 5px rgba(0,0,0,.2);
	transition:.5s;
}


.hamburger:before{
	top:-6px;
}

.hamburger:after{
	top:6px;
}

.icon.active .hamburger
{
	background: rgba(0,0,0,0);
	box-shadow:0 2px 5px rgba(0,0,0,.2);
}

.icon.active .hamburger:before{
	top:0;
	transform:rotate(45deg);
	
}

.icon.active .hamburger:after{
	top:0;
 transform:rotate(135deg);
}
	 
